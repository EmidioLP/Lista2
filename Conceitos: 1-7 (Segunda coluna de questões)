
def questao6():
    return 6*(1-2)

def questao7(a):
    juros = 10000*((1+(0.08/12))**(12*a))
    return juros

def questao8(r):

    return 3.14*float(r)**2

def questao9(a,b):
    return float(a)*float(b)

def questao10(q,l):
    cm = int(q)/float(l)
    return print("{:.3f} km/l".format(cm))
    
def questao11(c):
    return (c*1.8)+32

def questao12(f):
    return (f-32)/1.8
